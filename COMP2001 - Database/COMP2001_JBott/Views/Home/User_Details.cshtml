@{
    ViewData["Title"] = "User Details";
    string[] UsersHeaders = new string[] { "Email", "Username", "Password", "Admin" };
}

<h1>@ViewData["Title"]</h1>

<div class="table">
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                @{
                    foreach (var head in UsersHeaders)
                    {
                        <th>
                            @head
                        </th>
                    }
                }
            </tr>
        </thead>

        <tbody>
            @{
                if (Model != null)
                {
                    foreach (var Data in Model)
                    {
                        <tr>
                            <td>@Data.Email</td>
                            <td>@Data.Username</td>
                            <td>@Data.Password</td>
                            <td>@Data.Admin</td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>